<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />


  <%= f.label :artist_name %>
  <%= f.text_field :artist_name, id: :song_artist_name %>
  <br />


  <%= f.label :song_genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name, {}, {id: :song_genre_id} %><br />

  <%= f.label :song_notes_1 %>
  <%= f.text_field :note_contents, multiple: true, id: :song_notes_1 %>

  <%= f.label :song_notes_2 %>
  <%= f.text_field :note_contents, multiple: true, id: :song_notes_2 %>

  <%= f.submit %>
<% end %>
